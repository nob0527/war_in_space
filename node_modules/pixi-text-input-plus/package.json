{
  "name": "pixi-text-input-plus",
  "version": "1.0.27",
  "description": "PIXI text-input-plus",
  "main": "src/index.js",
  "types": "pixi-text-input-plus.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "buld": "npx webpack init",
    "watch": "webpack --watch",
    "patch:version": "npm --no-git-tag-version version patch && git commit -am \"update version to $npm_package_version\"",
    "minor:version": "npm --no-git-tag-version version minor && git commit -am \"update version to $npm_package_version\"",
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --mode=production",
    "build:prod:patch": "webpack --mode=production && npm run patch:version && git push origin master && npm publish",
    "build:prod:minor": "webpack --mode=production && npm run minor:version && git push origin master && npm publish",
    "start": "webpack serve --open --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lycwed/text-input-plus.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lycwed/text-input-plus/issues"
  },
  "homepage": "https://github.com/lycwed/text-input-plus#readme",
  "dependencies": {
    "pixi.js": "^5.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@webpack-cli/generators": "^2.0.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.1",
    "html-webpack-injector": "^1.1.4",
    "html-webpack-plugin": "^5.3.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
